record(ai, "$(P)readFloat64") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) TestLREAL LREAL 1 none none")
}

record(ao, "$(P)writeFloat64") {
    field(DTYP, "asynFloat64")
    field(OUT, "@asyn($(PORT), 0, 1)TestLREAL LREAL 1 none none")
}