#Test each datatype

record(bo, "$(P)writeBool") {
    field(DTYP, "asynUInt32Digital")
    field(OUT, "@asynMask($(PORT), 0, 1)testBOOL BOOL 1 0 0")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

record(longout, "$(P)writeInt16") {
    field(DTYP, "asynInt32")
    field(OUT, "@asyn($(PORT), 0, 1)transportArray1[$(I)] INT 1 0 0")
}

record(waveform, "$(P)writeInt16Array") {
    field(FTVL, "SHORT")
    field(DTYP, "asynInt16ArrayOut")
    field(INP, "@asyn($(PORT), 0, 1)intArray[1] INT 10 none none")
    field(NELM, "10")
}

