record(waveform, "$(P)readHeatingZones0") {
    field(SCAN, "I/O Intr")
    field(FTVL, "UCHAR")
    field(DTYP, "asynInt8ArrayIn")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) Zones[1] UDT 7 none &allow_packing=1")
    field(NELM, "1996")
}

