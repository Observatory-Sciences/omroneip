record(lsi, "$(P)readString") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) TestSTRING STRING 1 none none")
    field(SIZV, "41")
}

record(lso, "$(P)writeString") {
    field(DTYP, "asynOctetRead")
    field(OUT, "@asyn($(PORT), 0, 1)TestSTRING STRING 1 none none")
    field(SIZV, "41")
}
