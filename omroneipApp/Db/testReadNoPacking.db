#Packing does not effect the behaviour of this record.
record(waveform, "$(P)readUDTNoPacking") {
    field(SCAN, "I/O Intr")
    field(FTVL, "UCHAR")
    field(DTYP, "asynInt8ArrayIn")
    field(INP, "@asyn($(PORT), 0, 2)@$(POLLER) ISISTestMultiple[1] UDT 9 0 &allow_packing=0")
    field(NELM, "1996")
}

