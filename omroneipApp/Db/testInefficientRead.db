#Read three fields from struct without using offset (sends three CIP requests for the three items)

record(lsi, "$(P)$(R)readString1") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(PORT), 0, 2)@$(POLLER) ISISTestMultiple[$(I)].PVName STRING 1 none 0")
    field(SIZV, "61")
}

