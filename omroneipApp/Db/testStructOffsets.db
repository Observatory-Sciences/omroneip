#Get offset from within Time struct
record(longin, "$(P)readDInt1") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) testTimeStruct DINT 1 testTimeStruct2[2] &optimise=1")
}
#Get offset from within Time struct
record(ai, "$(P)readDouble") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) testTimeStruct LREAL 1 testTimeStruct2[5] &optimise=1")
}
record(ai, "$(P)readDouble2") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) myArrayStruct LREAL 1 testArrayStruct2[5][5][5] &optimise=1")
}

#Get offset from within Time struct embedded in another struct
#Offset 4 is an array of Time structs. Offset 0 is the first struct in the array. Offset 1 is the DINT within the struct
record(longin, "$(P)readDInt2") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) myArrayStruct DINT 1 testArrayStruct2[5][1][2] &optimise=1")
}

record(ai, "$(P)readFloat") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) myArrayStruct REAL 1 testArrayStruct2[3] &optimise=1")
}

#Read single bool from struct in array in struct
record(bi, "$(P)readBool1") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynUInt32Digital")
    field(INP, "@asynMask($(PORT), 0, 1)@$(POLLER) myArrayStruct BOOL 1 testArrayStruct2[5][2][1] &optimise=1")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

#Read a bool from a bool array within struct
record(bi, "$(P)readBool2") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynUInt32Digital")
    field(INP, "@asynMask($(PORT), 0, 1)@$(POLLER) myArrayStruct BOOL 1 testArrayStruct2[4][21] none")
    field(ZNAM, "Off")
    field(ONAM, "On")
}