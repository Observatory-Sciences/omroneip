record(longin, "$(P)readInt32") {
    field(SCAN, "I/O Intr")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT), 0, 1)@$(POLLER) TestDINT DINT 1 none none")
}

record(longout, "$(P)writeInt32") {
    field(DTYP, "asynInt32")
    field(OUT, "@asyn($(PORT), 0, 1)TestDINT DINT 1 none none")
}