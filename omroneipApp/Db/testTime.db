#Write records
record(ao, "$(P)writeDT") {
    field(DTYP, "asynInt64")
    field(OUT, "@asyn($(PORT), 0, 1)testTime TIME 1 none &allow_packing=0")
}

record(ao, "$(P)writeTIME") {
    field(DTYP, "asynInt64")
    field(OUT, "@asyn($(PORT), 0, 1)testTime2 TIME 1 none &allow_packing=0")
}

record(ao, "$(P)writeDATE") {
    field(DTYP, "asynInt64")
